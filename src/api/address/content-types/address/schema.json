{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "country": {
      "type": "string",
      "required": true,
      "default": "Portugal"
    },
    "city": {
      "type": "string",
      "default": "Porto",
      "required": true
    },
    "street": {
      "type": "string",
      "required": true
    },
    "postalCode": {
      "type": "string",
      "regex": "[0-9]{4}-[0-9]{3}",
      "required": true
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    }
  }
}
