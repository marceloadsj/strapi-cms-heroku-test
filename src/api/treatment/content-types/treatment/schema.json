{
  "kind": "collectionType",
  "collectionName": "treatments",
  "info": {
    "singularName": "treatment",
    "pluralName": "treatments",
    "displayName": "Treatment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "onGoing",
        "completed",
        "cancelled"
      ]
    },
    "startDate": {
      "type": "datetime",
      "required": true
    },
    "endDate": {
      "type": "datetime",
      "required": false
    },
    "notes": {
      "type": "text"
    },
    "partner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::partner.partner"
    },
    "price": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::price.price"
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "videos": {
      "allowedTypes": [
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
